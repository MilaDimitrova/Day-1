#include <stdio.h>
#include <stdlib.h>


int main()
{
    int n;
    printf("Please enter number: ");
    scanf("%d", &n);
    if(n % 2 == 1) {
        printf("Can't divide odd numbers by 2.");
        return -1;
    }
    while(n > 1) {
        if(n / 2 == (n-1) / 2) {
            printf("%d is not dividable by 2.", n);
            return -1;
        }
        n = n / 2;
    }
    printf("Yes,the operation can be done!");
    return 0;
}

